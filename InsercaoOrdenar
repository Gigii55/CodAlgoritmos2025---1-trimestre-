package Estudar;

import java.util.Arrays;

public class InsercaoOrdenar {

	public static void main(String[] args) {
		int vet[]= {4,6,7,1,2,3};
		for (int x = 1; x<=vet.length; x++) {
			int chave= vet[x];
			int y = x-1;
					
			while (y>=0 && vet[y]>chave) {
				vet [y+1]=  vet [y];
				y--;
			}
			vet[y+1] = chave;
			System.out.println(Arrays.toString(vet));


		}

	}

}
